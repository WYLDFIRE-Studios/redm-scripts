/*--------------------------------------------*/



.waves-effect {
    position: relative;
    cursor: pointer;
    display: inline-block;
    overflow: hidden;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-tap-highlight-color: transparent;
    vertical-align: middle;
    z-index: 1;
    -webkit-transition: .3s ease-out;
    transition: .3s ease-out;
}

.waves-light {
    padding: 15px 10px!important;
}
 
.horseStats {
    position: absolute;
    top: 500px;
    right: 100px;
    background-size: 100px 100px;
   
} 
 
.btn,
.btn-large,
.btn-small {
   
    display:flex !important;
    flex-direction: row!important;
    color: white;
    margin-top:0px;
    height:10px;
}

.btn:active {
    user-select: none;
  

}

.btn:hover {
     border: 4px solid white;
     border-radius: 5px; /* confimr and close*/
    
   
}
/*-------------------------------------------- icons hover -----------*/
.btn-small:hover {
    margin-left: 10px
}
/*--------------------------------------------------------------*/
.confirm{
    width: 350px;
    text-align: -webkit-center;
    margin-left:-60px
} 

.confirm-button{
    height: 2px;
    width: 30%;
    font-family: "RDR Lino";
    margin-top: 40px;
   

}

.confirm-button:hover{
   
    transition: background-color .2s ease-out;
    background-size: 100% 100% !important; 
}

button {
    outline: none !important;
    background-color: #ffffff00 !important;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;

   
}
.rotate{
  width: 350px;
  text-align: -webkit-center;
   font-family: "RDR Lino";

}

.rotate-button{
    height: 37px;
    width: 30%;
    margin-left:438px;
    margin-top: -35px;
    font-family: "RDR Lino";

    
}

.rotate-button:hover{
    border-radius: 5px;
    border: 4px solid white;
   
}



.close-button{
    font-family: "RDR Lino";
    margin-top: 60px;
    margin-left: 300px;
}

.collapsible-header {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    cursor: pointer;
    -webkit-tap-highlight-color: transparent;
    line-height: 1.5;
    padding: 1rem;
    background-color: transparent !important;
    border: 0 !important;
}




/*-------------------------------------------- input TACK-----*/
input {
    border: 0p solid  !important;
 
}

input[type='text'],
input[type='number'] {
    font-family: "RDR Lino";
    color: white;
    background-size: 100% 100%;
}

input[type='text']:focus,
input[type='number']:focus {
    color: #ffffff;
}

input[type='text']:hover,
input[type='number']:hover {
    color: #f4f4f4;
}

input[type='text']::-webkit-input-placeholder,
input[type='number']::-webkit-input-placeholder {
    color: #ffffff;
}

input[type='text']:-webkit-autofill,
input[type='number']:-webkit-autofill {
    color: black !important;
    background-color: transparent !important;
}

button {
    outline: none !important;
    background-color: transparent !important;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}


.collapsible-body {
    border: 0px !important;
}

.collapsible {
    border: 0px !important;
}

.collapsible li {
    display:flex;
    flex-direction: column;
    align-items: center;
}

.priceTable {
    border: 0;
}

.priceTable td {
    padding: 0.25rem;
    text-align: center;

    display:flex;
    flex-direction: column;
    align-items: center;
}




@media (max-width:1920px){

.header-text {
    margin-top:30px;  
}


.confirm-button{
    margin-top: 30px;
}

.close-button{
    margin-top: 10px;

}



.scroll-container {
 height: calc(100% - 330px);

  
}

 
.horseStats {
    width:500px;
} 



}