                         
@font-face { font-family: "RDR Lino"; src: url(fonts/rdolino.ttf); }


body {
    font-family: "RDR Lino";
    overflow: hidden;
    display: block;
    height: 100%;
    z-index: 999;
    min-width: 0;
}

* {
    user-select: none;
    /* transition: all 0.3s; */
}

div {
    display: block;
}

.scroll-container::-webkit-scrollbar {
   display: none;
}

.scroll-container::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgb(77 70 70);
    box-shadow: inset 0 0 6px rgb(77 70 70);
}

.scroll-container::-webkit-scrollbar-thumb {
    outline: 1px solid #fefbfb;
    border-radius: 5px;
}

input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    -moz-appearance: none;
    margin: 0;
}

input[type=number] {
    /* border: 0 !important; */
    -webkit-appearance: none;
    -moz-appearance: none;
    box-shadow: none !important;
}

input[type=number]:active {
    border-bottom-color: rgba(45, 146, 235, 0.9) !important;
}

button {
    color:white;
    border:0px;
}



.buy-buttons {
  display:grid;
  margin-top: -10px
}


.spacing2 {
    margin-bottom: 100px;

}

.spacing10 {
    margin-bottom: 100px;
}

.border-top {
    width: 100%;
    height: 1px;
    background-color: rgb(241 227 227);
    text-align: right;
}

#head {
    margin: 10 rem;
    min-width: 420px;
    border-radius: 2px;
    overflow: hidden;
    transition: all 0.5s;
}



.header-text {
    position: relative;
    margin-top:49px;
    font-size: 49px;
    color: #ffffff;
    font-family: "RDR Lino";
    text-align: center;
}






#clothes-container,
#creator-container,
#barber-container,
#tats-container {
    padding-bottom: 100px;
    overflow: hidden;
    position: absolute;
    height: 80%;
    left: 3%;
    top: 5%;
    width: 580px;
    color: #f6f6f6 !important;
    background: url('img/bgPanel.png');
    background-size: 100% 100%;
}

.scroll-container {

    padding: 15px !important;
    margin: 1px  !important;
    overflow-y: scroll;
    overflow-x: hidden;
    height: calc(100% - 408px);
    width: 100%;


}
/*------- menu buttons */
#button_menu {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    border-radius: 10px;
    padding: 25px 39px;
    height: 1vh;


}

.menu-selectb {
    flex-grow: 1;
    border-radius: 0px;
    margin-top: -10px;
    height: 5vh;
    border: 0px #fff solid;
    font-family: "RDR Lino";
    font-size: 20px;
    color: #3a3737;
  
  
}


.menu-selectb:hover {
    color: white!important;
   
}

.menu-selectb.active {
    color: #bb012d!important;
   
}
